{
  "version": 2,
  "builds": [
    {
      "src": "web/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "api/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ]
}
