{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest --detectOpenHandles",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/main.ts\" -e .ts",
    "dev:old": "ts-node-dev --transpile-only --no-notify ./src/main.ts",
    "start": "node ./dist/main.js",
    "build": "tsc",
    "lint": "tsc --noEmit && TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "db:seed": "npx prisma db seed",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio"
  },
  "devDependencies": {
    "@boost/module": "^4.0.0",
    "@faker-js/faker": "^8.2.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/schema-ast": "4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-apollo": "4.1.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.14.2",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.33.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "scripts": "*",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "*",
    "typescript": "^4.8.4",
    "zenstack": "^1.9.0"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "@pothos/core": "^3.22.5",
    "@pothos/plugin-errors": "^3.11.1",
    "@pothos/plugin-prisma": "^3.53.0",
    "@pothos/plugin-prisma-utils": "^0.14.0",
    "@pothos/plugin-relay": "^3.45.1",
    "@pothos/plugin-simple-objects": "^3.7.0",
    "@pothos/plugin-with-input": "^3.10.1",
    "@prisma/client": "^5.2.0",
    "@spotify/web-api-ts-sdk": "^1.2.0",
    "body-parser": "^1.19.2",
    "cloudinary": "^1.37.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.20.1",
    "jsonwebtoken": "^9.0.2",
    "logger": "*",
    "prisma": "^5.2.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "zenstack": {
    "schema": "./src/schema.zmodel"
  }
}
