{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"node -r esbuild-register ./src/main.ts\" -e .ts",
    "dev:codegen": "npm run dev",
    "start": "node dist/main.js",
    "build": "tsup",
    "lint": "tsc --noEmit && eslint ./src/**/*.ts --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "db:seed": "npx prisma db seed",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:deploy": "tsx ./src/db/migrate-deploy.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "@pothos/core": "^4.2.0",
    "@pothos/plugin-errors": "^3.11.1",
    "@pothos/plugin-prisma": "^3.53.0",
    "@pothos/plugin-prisma-utils": "^0.14.0",
    "@pothos/plugin-relay": "^3.45.1",
    "@pothos/plugin-simple-objects": "^3.7.0",
    "@pothos/plugin-tracing": "^1.1.0",
    "@pothos/plugin-with-input": "^3.10.1",
    "prisma": "5.18.0",
    "@prisma/client": "5.18.0",
    "@spotify/web-api-ts-sdk": "1.2.0",
    "apollo-opentracing": "^3.0.45",
    "axios": "^1.6.7",
    "body-parser": "^1.19.2",
    "cloudinary": "^1.37.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "graphql": "^16.8.1",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.20.1",
    "jsonwebtoken": "^9.0.2",
    "logger": "*",
    "newrelic": "^12.0.0",
    "runtime": "*"
  },
  "devDependencies": {
    "@boost/module": "^4.0.0",
    "@faker-js/faker": "^8.2.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/schema-ast": "4.1.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.2.3",
    "@graphql-codegen/typescript-react-apollo": "4.1.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/newrelic": "^9.14.4",
    "@types/node": "^18.14.2",
    "@typescript-eslint/parser": "8.6.0",
    "esbuild-register": "^3.3.2",
    "eslint": "8.57.0",
    "eslint-config-custom": "*",
    "nodemon": "^3.0.1",
    "prettier": "3.2.4",
    "scripts": "*",
    "ts-node-dev": "^2.0.0",
    "tsconfig-custom": "*",
    "tsup": "^8.2.3",
    "typescript": "5.5.4"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
