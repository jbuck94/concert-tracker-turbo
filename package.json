{
  "name": "turbo-photos-prisma-nextjs-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npx turbo run build",
    "start": "npx turbo start",
    "dev": "npx turbo run dev --parallel",
    "dev:web": "npx turbo run dev --filter=web",
    "dev:api": "npx turbo run dev --filter=api",
    "db:generate": "npx turbo run db:generate",
    "db:push": "npx turbo run db:push db:generate",
    "db:seed": "npx turbo run db:seed",
    "db:studio": "npx turbo run db:studio",
    "db:deploy": "npx turbo run db:deploy",
    "dev:codegen": "npx turbo run dev:codegen",
    "codegen": "npx turbo run codegen",
    "lint": "npx turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@typescript-eslint/parser": "8.6.0",
    "eslint": "8.57.0",
    "eslint-config-custom": "*",
    "prettier": "3.2.4",
    "turbo": "2.0.9",
    "tsx": "4.10.2",
    "typescript": "5.5.4"
  },
  "packageManager": "npm@10.8.2"
}
