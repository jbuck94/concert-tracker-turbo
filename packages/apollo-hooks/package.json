  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript-operations": "4.2.3",
    "@graphql-codegen/typescript-react-apollo": "4.1.0",
    "@types/jest": "^29.5.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.0",
    "jest": "^29.5.0",
    "react": "^18.2.0",
    "scripts": "*",
    "tsconfig": "*",
    "tsup": "^6.1.2",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "scripts/jest/node"
  }
}
